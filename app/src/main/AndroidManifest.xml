<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pe.bonifacio.pasapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activities.ReporteActivity"></activity>
        <activity android:name=".activities.TipoActivity" />
        <activity android:name=".activities.SuperficieActivity" />
        <activity android:name=".activities.ListaSuperficieActivity" />
        <activity android:name=".activities.ListaMinaActivity" />
        <activity android:name=".activities.MinaActivity" />
        <activity android:name=".activities.ListaProyectoActivity" />
        <activity android:name=".activities.ProyectoActivity" />
        <activity android:name=".activities.ProfileActivity" />
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.SignUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>